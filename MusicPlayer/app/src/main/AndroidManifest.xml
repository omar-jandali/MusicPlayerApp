<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.janda_000.musicplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchActivity" android:label="Search" />
        <activity android:name=".BrowseActivity" android:label="Browse"/>
        <activity android:name=".LibraryActivity" android:label="Library"/>
        <activity android:name=".PlayingActivity" android:label="Playing"/>
        <activity android:name=".RecommendedActivity" android:label="Recommended"/>
    </application>

</manifest>